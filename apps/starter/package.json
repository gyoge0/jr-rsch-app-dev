{
  "name": "@acme/starter",
  "private": true,
  "license": "Apache-2.0",
  "author": "Yogesh Thambidurai <yogesh@gyoge.com>",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "expo start --go",
    "lint": "eslint --ext js,jsx .",
    "build": "expo export --output-dir ./build --platform all",
    "start": "expo start --go",
    "android": "expo start --go --android",
    "ios": "expo start --go --ios",
    "web": "expo start --go --web",
    "eas-build-post-install": "pnpm run -w build:starter"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.14",
    "@react-navigation/native-stack": "^6.9.24",
    "expo": "~50.0.11",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.24.11",
    "react": "18.2.0",
    "react-native": "0.73.5",
    "react-native-gesture-handler": "~2.15.0",
    "react-native-reanimated": "~3.7.2",
    "react-native-safe-area-context": "4.9.0",
    "react-native-screens": "~3.29.0"
  },
  "devDependencies": {
    "@acme/eslint-config": "*",
    "@babel/core": "^7.24.0",
    "@types/react-native": "^0.73.0",
    "babel-preset-expo": "^10.0.1"
  },
  "eslintConfig": {
    "extends": "@acme/eslint-config",
    "ignorePatterns": [
      "node_modules",
      "build",
      ".expo",
      ".expo-shared"
    ],
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "env": {
          "node": true
        }
      }
    ]
  }
}
