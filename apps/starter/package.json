{
  "name": "@acme/starter",
  "private": true,
  "license": "Apache-2.0",
  "author": "Yogesh Thambidurai <yogesh@gyoge.com>",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "expo start --go",
    "lint": "eslint --ext js,jsx .",
    "build": "expo export --output-dir ./build --platform all",
    "start": "expo start --go",
    "android": "expo start --go --android",
    "ios": "expo start --go --ios",
    "web": "expo start --go --web",
    "eas-build-post-install": "pnpm run -w build:starter"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.11",
    "expo": "^49.0.22",
    "expo-dev-client": "^2.4.12",
    "expo-status-bar": "^1.11.1",
    "expo-updates": "^0.18.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-reanimated": "^3.6.2",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "^3.29.0",
    "react-navigation": "^5.0.0",
    "react-navigation-stack": "^2.10.4"
  },
  "devDependencies": {
    "@acme/eslint-config": "^0.0.0",
    "@babel/core": "^7.23.9",
    "@types/react": "^18.2.51",
    "@types/react-dom": "^18.2.18",
    "babel-preset-expo": "^9.9.0"
  },
  "eslintConfig": {
    "extends": "@acme/eslint-config",
    "ignorePatterns": [
      "node_modules",
      "build",
      ".expo",
      ".expo-shared"
    ],
    "overrides": [
      {
        "files": [
          "*.js"
        ],
        "env": {
          "node": true
        }
      }
    ]
  }
}
