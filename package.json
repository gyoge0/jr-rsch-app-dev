{
  "name": "@gyoge/jr-rsch-app-dev",
  "private": true,
  "license": "Apache-2.0",
  "author": "Yogesh Thambidurai <yogesh@gyoge.com>",
  "version": "0.0.0",
  "scripts": {
    "dev": "turbo dev",
    "dev:starter": "turbo dev --filter=\"{./apps/starter}...\"",
    "dev:foodie": "turbo dev --filter=\"{./apps/foodie}...\"",
    "dev:weather": "turbo dev --filter=\"{./apps/weather}...\"",
    "lint": "turbo lint",
    "test": "turbo test",
    "build": "turbo build",
    "build:starter": "turbo build --filter=\"...{./apps/starter}\"",
    "build:foodie": "turbo build --filter=\"...{./apps/foodie}\"",
    "build:weather": "turbo build --filter=\"...{./apps/weather}\"",
    "format": "prettier --write .",
    "format:dry": "prettier ."
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^1.13.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "expo-modules-*"
      ]
    }
  }
}
